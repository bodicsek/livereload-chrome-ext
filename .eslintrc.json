{
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "quotes": [
      "error",
      "double", {
        "allowTemplateLiterals": true
      }
    ],
    "no-console": "off",
    "strict": [
      "error",
      "global"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
